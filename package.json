{
  "name": "3h-dict",
  "version": "0.1.0",
  "description": "A utility lib for language switch in vue.",
  "type": "module",
  "module": "./dist/3h-dict.min.js",
  "main": "./dist/3h-dict.umd.min.js",
  "types": "./index.d.ts",
  "author": "hhh",
  "license": "MIT",
  "scripts": {
    "prebuild": "tsc",
    "build": "rollup -c",
    "postbuild": "terser dist/3h-dict.js --config-file terser.json --module -o dist/3h-dict.min.js && terser dist/3h-dict.umd.js --config-file terser.json -o dist/3h-dict.umd.min.js && node stat.cjs",
    "docs": "dts2md \"**\" \"!index.d.ts\" -i ./types -o ./docs -l -I Home.md"
  },
  "repository": "huang2002/3h-dict",
  "keywords": [
    "dict",
    "i18n",
    "l10n",
    "intl",
    "internationalization",
    "language",
    "switch",
    "select",
    "vue",
    "3h"
  ],
  "files": [
    "dist",
    "types",
    "index.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^6.0.0",
    "dts2md": "^0.4.0",
    "rollup": "^3.7.0",
    "terser": "^5.16.0",
    "typescript": "^4.9.0",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@vue/reactivity": "^3.2.45"
  }
}
